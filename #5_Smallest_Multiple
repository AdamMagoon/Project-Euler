myList = [11,17,19,13,16,12,14,15,18,20]

i = 2520
found = False
while found == False:
    for x in myList:
        if x == 20:
            print('The smallest multiple is', i)
            found = True
            break
        elif i % x == 0:
            continue
        else:
            i += 20
            break
